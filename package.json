{
  "name": "jetbrains-plugin-platform-version-update-action",
  "version": "0.0.2",
  "private": true,
  "description": "GitHub Action to update JetBrains Plugins when a new JetBrains Platform Version is available.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "engines": {
    "node": ">= 16.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisCarini/gh-test-ij-release-update-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.4.0",
    "@actions/http-client": "^2.2.1",
    "@octokit/webhooks-types": "^7.3.2",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "properties": "^1.2.1",
    "semver": "^7.6.2",
    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/parser": "^7.14.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^5.0.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "3.3.2",
    "ts-jest": "^28.0.8",
    "typescript": "^5.5.2"
  },
  "volta": {
    "node": "20.11.0"
  }
}
