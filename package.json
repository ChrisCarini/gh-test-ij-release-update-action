{
  "name": "jetbrains-plugin-platform-version-update-action",
  "description": "GitHub Action to update JetBrains Plugins when a new JetBrains Platform Version is available.",
  "version": "0.0.2",
  "author": "ChrisCarini <6374067+chriscarini@users.noreply.github.com>",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/ChrisCarini/gh-test-ij-release-update-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisCarini/gh-test-ij-release-update-action.git"
  },
  "bugs": {
    "url": "https://github.com/ChrisCarini/gh-test-ij-release-update-action/issues"
  },
  "keywords": [
    "actions"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts --out dist --source-map --license licenses.txt",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.3",
    "@actions/exec": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@actions/glob": "^0.6.1",
    "@actions/http-client": "^4.0.0",
    "@octokit/webhooks-types": "^7.6.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "properties": "^1.2.1",
    "semver": "^7.7.4",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@github/local-action": "^7.0.0",
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.8.1",
    "prettier-eslint": "^16.4.2",
    "rollup": "^4.57.0",
    "ts-jest": "^29.4.6",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  },
  "volta": {
    "node": "24.5.0"
  }
}
